<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xsi:schemaLocation="NLog NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      internalLogFile="C:\Logs\Internal-NLog\NLogAsMicrosoftProxy-Sample.internal.log"
      internalLogLevel="Info" >

  <!-- the targets to write to -->
  <targets>
    <target xsi:type="File"
            name="logfile"
            fileName="C:\Logs\NLogAsMicrosoftProxy-Sample.log"
            keepFileOpen="true"
            openFileCacheTimeout="30"
            archiveEvery="Day"
            archiveDateFormat="yyyy-MM-dd"
            archiveNumbering="Date"
            maxArchiveDays="10"
            encoding="utf-8"
            layout="${longdate} ${level:padding=5} [${activityid}] [${callsite}]: ${message} ${when:when=length('${all-event-properties}')>0:Inner:| ${all-event-properties}}${newline:when=length('${exception}')>0}${exception:format=tostring}" />
    <target xsi:type="ColoredConsole"
            name="logconsole"
            layout="${longdate} ${level:padding=5} [${activityid}] [${callsite}]: ${message} ${when:when=length('${all-event-properties}')>0:Inner:| ${all-event-properties}}${newline:when=length('${exception}')>0}${exception:format=tostring}" />
  </targets>

  <!-- rules to map from logger name to target -->
  <rules>
    <logger name="*" minlevel="Trace" writeTo="logfile,logconsole" />
  </rules>
</nlog>