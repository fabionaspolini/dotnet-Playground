<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>MicrosoftLoggingWebApi_Sample</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\microsoft-logging\MyLogger\MyConsoleLoggerFormatter.cs" Link="MyLogger\MyConsoleLoggerFormatter.cs" />
    <Compile Include="..\microsoft-logging\MyLogger\MyConsoleLoggerFormatterHelpers.cs" Link="MyLogger\MyConsoleLoggerFormatterHelpers.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="7.0.7" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="MyLogger\" />
  </ItemGroup>

</Project>
