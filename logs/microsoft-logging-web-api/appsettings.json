{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      //"microsoft-logging-web-api_Sample.Controllers": "Error",
      "Payloads": "None"
    },
    "Console": {
      //"LogLevel": {
      //  "Default": "Information",
      //  "Microsoft": "Warning",
      //  "Microsoft.Hosting.Lifetime": "Information"
      //},
      //"FormatterName": "console",
      //"FormatterName": "json",
      "FormatterName": "MyConsoleLoggerFormatter",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "O",
        "UseUtcTimestamp": false,
        "JsonWriterOptions": {
          "Indented": true
          //"Encoder": "UnsafeRelaxedEscapingSingleton" // NÃ£o localizei como atribuitp via appsettings
        }
      }
    }
  },
  "AllowedHosts": "*"
}
